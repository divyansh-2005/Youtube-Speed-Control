{
    "manifest_version": 3,
    "name": "YouTube Custom Playback Speed",
    "version": "1.0",
    "description": "Allows custom YouTube playback speeds",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
  